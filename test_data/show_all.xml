<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Sequence>
            <Fallback>
                <RandomSelector>
                    <Repeat num_cycles="1">
                        <AlwaysFailure/>
                    </Repeat>
                </RandomSelector>
            </Fallback>
            <PersistentSequence name="DoPersistentSequence">
                <RetryUntilSuccesful num_attempts="1">
                    <BlackboardCheckString value_A="" return_on_mismatch="" value_B="">
                        <AlwaysSuccess/>
                    </BlackboardCheckString>
                </RetryUntilSuccesful>
            </PersistentSequence>
            <Inverter>
                <ForceFailure>
                    <ForceSuccess>
                        <SetBlackboard value="value" output_key=""/>
                    </ForceSuccess>
                </ForceFailure>
            </Inverter>
            <RandomSequence>
                <Action ID="Pippo"/>
            </RandomSequence>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="Pippo"/>
    </TreeNodesModel>
    <!-- ////////// -->
</root>

